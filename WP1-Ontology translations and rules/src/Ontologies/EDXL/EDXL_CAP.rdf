    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#addresses -->

    <owl:DatatypeProperty rdf:about="&edxl_cap;addresses">
        <rdfs:comment xml:lang="en">The group listing of intended recipients of the alert message</rdfs:comment>
        <rdfs:domain rdf:resource="&edxl_cap;Scope"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#areaAltitude -->

    <owl:DatatypeProperty rdf:about="&edxl_cap;areaAltitude">
        <rdfs:comment xml:lang="en">The specific or minimum altitude of the affected area of the alert message</rdfs:comment>
        <rdfs:domain rdf:resource="&edxl_cap;Area"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#areaCeiling -->

    <owl:DatatypeProperty rdf:about="&edxl_cap;areaCeiling">
        <rdfs:comment xml:lang="en">The maximum altitude of the affected area of the alert message</rdfs:comment>
        <rdfs:domain rdf:resource="&edxl_cap;Area"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#areaDesc -->

    <owl:DatatypeProperty rdf:about="&edxl_cap;areaDesc">
        <rdfs:comment xml:lang="en">The text describing the affected area of the alert message</rdfs:comment>
        <rdfs:domain rdf:resource="&edxl_cap;Area"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#areaGeoCode -->

    <owl:DatatypeProperty rdf:about="&edxl_cap;areaGeoCode">
        <rdfs:comment xml:lang="en">The geographic code delineating the affected area of the alert message</rdfs:comment>
        <rdfs:domain rdf:resource="&edxl_cap;Area"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#audience -->

    <owl:DatatypeProperty rdf:about="&edxl_cap;audience">
        <rdfs:comment xml:lang="en">The text describing the intended audience of the alert message.</rdfs:comment>
        <rdfs:domain rdf:resource="&edxl_cap;Info"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#certainty -->

    <owl:DatatypeProperty rdf:about="&edxl_cap;certainty">
        <rdfs:comment xml:lang="en">The code denoting the certainty of the subject event of the alert message</rdfs:comment>
        <rdfs:domain rdf:resource="&edxl_cap;Info"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#code -->

    <owl:DatatypeProperty rdf:about="&edxl_cap;code">
        <rdfs:comment xml:lang="en">The code denoting the special handling of the alert message</rdfs:comment>
        <rdfs:domain rdf:resource="&edxl_cap;AlertMessage"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#contact -->

    <owl:DatatypeProperty rdf:about="&edxl_cap;contact">
        <rdfs:comment xml:lang="en">The text describing the contact for follow-up and confirmation of the alert message</rdfs:comment>
        <rdfs:domain rdf:resource="&edxl_cap;Info"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#dateTime -->

    <owl:DatatypeProperty rdf:about="&edxl_cap;dateTime">
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#derefUri -->

    <owl:DatatypeProperty rdf:about="&edxl_cap;derefUri">
        <rdfs:comment xml:lang="en">The base-64 encoded data content of the resource file</rdfs:comment>
        <rdfs:domain rdf:resource="&edxl_cap;Resource"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#description -->

    <owl:DatatypeProperty rdf:about="&edxl_cap;description">
        <rdfs:comment xml:lang="en">The text describing the subject event of the alert message</rdfs:comment>
        <rdfs:domain rdf:resource="&edxl_cap;Info"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#digest -->

    <owl:DatatypeProperty rdf:about="&edxl_cap;digest">
        <rdfs:comment xml:lang="en">The code representing the digital digest (“hash”) computed from the resource file</rdfs:comment>
        <rdfs:domain rdf:resource="&edxl_cap;Resource"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#effective -->

    <owl:DatatypeProperty rdf:about="&edxl_cap;effective">
        <rdfs:comment xml:lang="en">The effective time of the information of the alert message</rdfs:comment>
        <rdfs:domain rdf:resource="&edxl_cap;Info"/>
        <rdfs:subPropertyOf rdf:resource="&edxl_cap;dateTime"/>
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#event -->

    <owl:DatatypeProperty rdf:about="&edxl_cap;event">
        <rdfs:comment xml:lang="en">The text denoting the type of the subject event of the alert message</rdfs:comment>
        <rdfs:domain rdf:resource="&edxl_cap;Info"/>
    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#eventCode -->

    <owl:DatatypeProperty rdf:about="&edxl_cap;eventCode">
        <rdfs:comment xml:lang="en">A system- specific code identifying the event type of the alert message</rdfs:comment>
        <rdfs:domain rdf:resource="&edxl_cap;Info"/>
    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#expires -->

    <owl:DatatypeProperty rdf:about="&edxl_cap;expires">
        <rdfs:comment xml:lang="en">The expiry time of the information of the alert message</rdfs:comment>
        <rdfs:domain rdf:resource="&edxl_cap;Info"/>
        <rdfs:subPropertyOf rdf:resource="&edxl_cap;dateTime"/>
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#headline -->

    <owl:DatatypeProperty rdf:about="&edxl_cap;headline">
        <rdfs:comment xml:lang="en">A brief human-readable headline. Note that some displays (for example, short messaging service devices) may only present this headline; it SHOULD be made as direct and actionable as possible while remaining short.</rdfs:comment>
        <rdfs:domain rdf:resource="&edxl_cap;Info"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#identifier -->

    <owl:DatatypeProperty rdf:about="&edxl_cap;identifier">
        <rdfs:comment xml:lang="en">The identifier of the alert message</rdfs:comment>
        <rdfs:domain rdf:resource="&edxl_cap;AlertMessage"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#instruction -->

    <owl:DatatypeProperty rdf:about="&edxl_cap;instruction">
        <rdfs:comment xml:lang="en">The text describing the recommended action to be taken by recipients of the alert message</rdfs:comment>
        <rdfs:domain rdf:resource="&edxl_cap;Info"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#language -->

    <owl:DatatypeProperty rdf:about="&edxl_cap;language">
        <rdfs:comment xml:lang="en">The code denoting the language of the info sub- element of the alert message</rdfs:comment>
        <rdfs:domain rdf:resource="&edxl_cap;Info"/>
        <rdfs:range rdf:resource="&xsd;language"/>
    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#mimeType -->

    <owl:DatatypeProperty rdf:about="&edxl_cap;mimeType">
        <rdfs:comment xml:lang="en">The identifier of the MIME content type and sub-type describing the resource file</rdfs:comment>
        <rdfs:domain rdf:resource="&edxl_cap;Resource"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#note -->

    <owl:DatatypeProperty rdf:about="&edxl_cap;note">
        <rdfs:comment xml:lang="en">The text describing the purpose or significance of the alert message</rdfs:comment>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&edxl_cap;ErrorType"/>
                    <rdf:Description rdf:about="&edxl_cap;Exercise"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#onset -->

    <owl:DatatypeProperty rdf:about="&edxl_cap;onset">
        <rdfs:comment xml:lang="en">The expected time of the beginning of the subject event of the alert message</rdfs:comment>
        <rdfs:domain rdf:resource="&edxl_cap;Info"/>
        <rdfs:subPropertyOf rdf:resource="&edxl_cap;dateTime"/>
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#parameter -->

    <owl:DatatypeProperty rdf:about="&edxl_cap;parameter">
        <rdfs:comment xml:lang="en">A system- specific additional parameter associated with the alert message</rdfs:comment>
        <rdfs:domain rdf:resource="&edxl_cap;Info"/>
    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#resourceDesc -->

    <owl:DatatypeProperty rdf:about="&edxl_cap;resourceDesc">
        <rdfs:comment xml:lang="en">The text describing the type and content of the resource file</rdfs:comment>
        <rdfs:domain rdf:resource="&edxl_cap;Resource"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#resourceSize -->

    <owl:DatatypeProperty rdf:about="&edxl_cap;resourceSize">
        <rdfs:comment xml:lang="en">The integer indicating the size of the resource file</rdfs:comment>
        <rdfs:domain rdf:resource="&edxl_cap;Resource"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#resourceURI -->

    <owl:DatatypeProperty rdf:about="&edxl_cap;resourceURI">
        <rdfs:comment xml:lang="en">The identifier of the hyperlink for the resource file</rdfs:comment>
        <rdfs:domain rdf:resource="&edxl_cap;Resource"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#restriction -->

    <owl:DatatypeProperty rdf:about="&edxl_cap;restriction">
        <rdfs:comment xml:lang="en">The text describing the rule for limiting distribution of the restricted alert message</rdfs:comment>
        <rdfs:domain rdf:resource="&edxl_cap;Restricted"/>
        <rdfs:range rdf:resource="&xsd;string"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topDataProperty"/>
    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#senderName -->

    <owl:DatatypeProperty rdf:about="&edxl_cap;senderName">
        <rdfs:domain rdf:resource="&edxl_cap;Sender"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#sentTime -->

    <owl:DatatypeProperty rdf:about="&edxl_cap;sentTime">
        <rdfs:comment xml:lang="en">The time and date of the origination of the alert message</rdfs:comment>
        <rdfs:domain rdf:resource="&edxl_cap;AlertMessage"/>
        <rdfs:subPropertyOf rdf:resource="&edxl_cap;dateTime"/>
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#source -->

    <owl:DatatypeProperty rdf:about="&edxl_cap;source">
        <rdfs:comment xml:lang="en">The text identifying the source of the alert message</rdfs:comment>
        <rdfs:domain rdf:resource="&edxl_cap;AlertMessage"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#webInfo -->

    <owl:DatatypeProperty rdf:about="&edxl_cap;webInfo">
        <rdfs:comment xml:lang="en">The identifier of the hyperlink associating additional information with the alert message</rdfs:comment>
        <rdfs:domain rdf:resource="&edxl_cap;Info"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#AckType -->

    <owl:Class rdf:about="&edxl_cap;AckType">
        <rdfs:subClassOf rdf:resource="&edxl_cap;MsgType"/>
        <owl:disjointWith rdf:resource="&edxl_cap;AlertType"/>
        <owl:disjointWith rdf:resource="&edxl_cap;CancelType"/>
        <owl:disjointWith rdf:resource="&edxl_cap;ErrorType"/>
        <owl:disjointWith rdf:resource="&edxl_cap;UpdateType"/>
        <rdfs:comment xml:lang="en">Acknowledges receipt and acceptance of the message(s) identified in &lt;references&gt;.</rdfs:comment>
        <rdfs:comment xml:lang="pt">Reconhece a recepção e aceitação da mensagem identificada previamente.</rdfs:comment>
    </owl:Class>
    


