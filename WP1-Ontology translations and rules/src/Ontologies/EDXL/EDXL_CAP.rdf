        <rdfs:range rdf:resource="&edxl_cap;MsgType"/>
    </owl:ObjectProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#hasReferences -->

    <owl:ObjectProperty rdf:about="&edxl_cap;hasReferences">
