        <rdfs:range rdf:resource="&edxl_cap;Severity"/>
    </owl:ObjectProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#hasStatus -->

    <owl:ObjectProperty rdf:about="&edxl_cap;hasStatus">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="&edxl_cap;AlertMessage"/>
        <rdfs:range rdf:resource="&edxl_cap;Status"/>
    </owl:ObjectProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#hasUrgency -->

    <owl:ObjectProperty rdf:about="&edxl_cap;hasUrgency">
        <rdfs:domain rdf:resource="&edxl_cap;Info"/>
        <rdfs:range rdf:resource="&edxl_cap;Urgency"/>
    </owl:ObjectProperty>
    


