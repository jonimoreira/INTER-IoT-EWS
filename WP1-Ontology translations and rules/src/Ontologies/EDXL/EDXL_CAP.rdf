    </owl:ObjectProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#isCertaintyOf -->

    <owl:ObjectProperty rdf:about="&edxl_cap;isCertaintyOf">
        <owl:inverseOf rdf:resource="&edxl_cap;hasCertainty"/>
    </owl:ObjectProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#isIncidentRelatedTo -->

    <owl:ObjectProperty rdf:about="&edxl_cap;isIncidentRelatedTo">
        <owl:inverseOf rdf:resource="&edxl_cap;hasIncidentRelated"/>
    </owl:ObjectProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#isInfoOf -->

    <owl:ObjectProperty rdf:about="&edxl_cap;isInfoOf">
        <owl:inverseOf rdf:resource="&edxl_cap;hasInfo"/>
    </owl:ObjectProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#isMsgTypeOf -->

    <owl:ObjectProperty rdf:about="&edxl_cap;isMsgTypeOf">
        <owl:inverseOf rdf:resource="&edxl_cap;hasMsgType"/>
    </owl:ObjectProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#isReferencedBy -->

    <owl:ObjectProperty rdf:about="&edxl_cap;isReferencedBy"/>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#isResourceOf -->

    <owl:ObjectProperty rdf:about="&edxl_cap;isResourceOf"/>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#isResponseTypeOf -->

    <owl:ObjectProperty rdf:about="&edxl_cap;isResponseTypeOf">
