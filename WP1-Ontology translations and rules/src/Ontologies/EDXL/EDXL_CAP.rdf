<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:edxl_cap="http://fpc.ufba.br/ontologies/edxl_cap#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:edxl_cap_inst="http://fpc.ufba.br/ontologies/edxl_cap/instances#"
  xml:base="http://fpc.ufba.br/ontologies/edxl_cap">
  <owl:Ontology rdf:about="">
    <edxl_cap:contributor xml:lang="en">Reinaldo Almeida (UFBA)</edxl_cap:contributor>
    <edxl_cap:contributor xml:lang="en">Rebeca Barros (UFBA)</edxl_cap:contributor>
    <edxl_cap:contributor xml:lang="en">Lais Salvador (UFBA)</edxl_cap:contributor>
    <edxl_cap:status xml:lang="en">An ongoing ontology for communication interoperability between systems in the context of the RESCUER Project. This propose is based on EDXL documentation (https://www.oasis-open.org/committees/tc_home.php?wg_abbrev=emergency).</edxl_cap:status>
    <edxl_cap:rights xml:lang="en">(c) The RESCUER project - http://www.rescuer-project.org/</edxl_cap:rights>
  </owl:Ontology>
  <owl:Class rdf:ID="Info">
    <rdfs:comment xml:lang="pt">Refere-se a todos os componentes de informações sobre a mensagem de alerta.</rdfs:comment>
    <rdfs:comment xml:lang="en">Refers to all component parts of the info sub-element of the alert message.

</rdfs:comment>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:someValuesFrom>
              <owl:Class rdf:ID="Area"/>
            </owl:someValuesFrom>
            <owl:onProperty>
              <owl:ObjectProperty rdf:ID="hasArea"/>
            </owl:onProperty>
          </owl:Restriction>
          <owl:Restriction>
            <owl:someValuesFrom>
              <owl:Class rdf:ID="Resource"/>
            </owl:someValuesFrom>
            <owl:onProperty>
              <owl:ObjectProperty rdf:ID="hasResource"/>
            </owl:onProperty>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:ID="Severity">
    <rdfs:comment xml:lang="en">The severity of the subject incident or event.
</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="UrgencyUnknown">
    <rdfs:comment xml:lang="pt">Urgência desconhecida</rdfs:comment>
    <rdfs:comment xml:lang="en">Urgency not known</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Urgency"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="AlertType">
    <rdfs:comment xml:lang="pt">Informação inicial requerendo atenção para os recipientes alvo.</rdfs:comment>
    <rdfs:comment xml:lang="en">Initial information requiring attention by targeted recipients.</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:ID="UpdateType"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="ErrorType"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="CancelType"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="MsgType"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Status">
    <rdfs:comment xml:lang="pt">Refere-se ao modo apropriado para lidar com o tipo de mensagem de alerta.</rdfs:comment>
    <rdfs:comment xml:lang="en">Refers to the appropriate handling of the alert message. </rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="Test">
    <rdfs:comment xml:lang="pt">Testes técnicos, todos os recipientes não são levados em consideração.</rdfs:comment>
    <rdfs:comment xml:lang="en">Technical testing only, all recipients disregard.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Status"/>
  </owl:Class>
  <owl:Class rdf:ID="Actual">
    <rdfs:comment xml:lang="pt">Acionável por todos os recipientes-alvo.</rdfs:comment>
    <rdfs:comment xml:lang="en">Actionable by all targeted recipients</rdfs:comment>
    <owl:disjointWith rdf:resource="#Test"/>
    <owl:disjointWith>
      <owl:Class rdf:ID="System"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Exercise"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Draft"/>
    </owl:disjointWith>
    <rdfs:subClassOf rdf:resource="#Status"/>
  </owl:Class>
  <owl:Class rdf:ID="Rescue">
    <rdfs:comment xml:lang="pt">Resgate e recuperação</rdfs:comment>
    <rdfs:comment xml:lang="en">Rescue and recovery</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Category"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Sender">
    <rdfs:comment xml:lang="en">The originator of an alert. Guaranteed by assigner to be unique globally; e.g., may be based on an Internet domain name.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#Exercise">
    <rdfs:comment xml:lang="pt">Acionável apenas por participantes designados ao exercício. Um identificador do exercício deve ser incluído.</rdfs:comment>
    <rdfs:comment xml:lang="en">Actionable only by designated exercise participants; exercise identifier SHOULD appear in &lt;note&gt;</rdfs:comment>
    <owl:disjointWith rdf:resource="#Test"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#System"/>
    </owl:disjointWith>
    <rdfs:subClassOf rdf:resource="#Status"/>
  </owl:Class>
  <owl:Class rdf:ID="Prepare">
    <rdfs:comment xml:lang="pt">Fazer preparações baseadas em instruções</rdfs:comment>
    <rdfs:comment xml:lang="en">Make preparations per the &lt;instruction&gt;</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:ID="Shelter"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="ResponseType"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Restricted">
    <rdfs:comment xml:lang="pt">Para disseminação somente para usuários com requerimento operacional conhecido.</rdfs:comment>
    <rdfs:comment xml:lang="en">For dissemination only to users with a known operational requirement. </rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Scope"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Area">
    <rdfs:comment xml:lang="pt">Refere-se a todos os componentes de area do sub-elemento Info do elemento Alerta.</rdfs:comment>
    <rdfs:comment xml:lang="en">Refers to all component parts of the area subelement of the info subelement of the 
alert message.

</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="Certainty">
    <rdfs:comment xml:lang="en">The certainty of the subject event of the alert message</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="Monitor">
    <rdfs:comment xml:lang="pt">Preocupar-se com fontes de informações como descritas em instruções
</rdfs:comment>
    <rdfs:comment xml:lang="en">Attend to information sources as described in &lt;instruction&gt;</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:about="#Shelter"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Prepare"/>
    <owl:disjointWith>
      <owl:Class rdf:ID="None"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#ResponseType"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Minor">
    <rdfs:comment xml:lang="pt">Ameaça mínima a vida ou propriedade.</rdfs:comment>
    <rdfs:comment xml:lang="en">Minimal threat to life or property</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:ID="SeverityUnknown"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Severe"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Moderate"/>
    </owl:disjointWith>
    <rdfs:subClassOf rdf:resource="#Severity"/>
  </owl:Class>
  <owl:Class rdf:ID="Avoid">
    <rdfs:comment xml:lang="en">Avoid the subject event as per the &lt;instruction&gt;</rdfs:comment>
    <rdfs:comment xml:lang="pt">Evitar o assunto de um evento de acordo a instruções</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:ID="Evacuate"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#None"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#ResponseType"/>
    </rdfs:subClassOf>
    <owl:disjointWith rdf:resource="#Monitor"/>
    <owl:disjointWith rdf:resource="#Prepare"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Shelter"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Execute"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:about="#None">
    <rdfs:comment xml:lang="pt">Nenhuma ação recomendada</rdfs:comment>
    <rdfs:comment xml:lang="en">No action recommended</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:about="#Shelter"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Prepare"/>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#ResponseType"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Draft">
    <rdfs:comment xml:lang="pt">Um rascunho preliminar, não acionável em sua forma atual.</rdfs:comment>
    <rdfs:comment xml:lang="en">A preliminary template or draft, not actionable in its current form.</rdfs:comment>
    <owl:disjointWith rdf:resource="#Test"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#System"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Exercise"/>
    <rdfs:subClassOf rdf:resource="#Status"/>
  </owl:Class>
  <owl:Class rdf:ID="Private">
    <rdfs:comment xml:lang="pt">Para disseminação apenas para endereços especificados.</rdfs:comment>
    <rdfs:comment xml:lang="en">For dissemination only to specified addresses.</rdfs:comment>
    <owl:disjointWith rdf:resource="#Restricted"/>
    <owl:disjointWith>
      <owl:Class rdf:ID="Public"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Scope"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Polygon">
    <rdfs:comment xml:lang="en">The paired values of points defining a polygon that delineates the affected area of the alert message</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Area"/>
  </owl:Class>
  <owl:Class rdf:ID="Geo">
    <rdfs:comment xml:lang="pt">Geofísica (ex: deslizamento)</rdfs:comment>
    <rdfs:comment xml:lang="en">Geophysical (inc. landslide)

</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Category"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Security">
    <rdfs:comment xml:lang="pt">Aplicações da lei, militares, segurança nacional e local/privada</rdfs:comment>
    <rdfs:comment xml:lang="en">Law enforcement, military, homeland and local/private security</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Category"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Fire">
    <rdfs:comment xml:lang="pt">Extinção de incêndio e resgate.</rdfs:comment>
    <rdfs:comment xml:lang="en">Fire suppression and rescue</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Category"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Unlikely">
    <rdfs:comment xml:lang="en">Not expected to occur (p ~ 0)</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Certainty"/>
  </owl:Class>
  <owl:Class rdf:about="#UpdateType">
    <rdfs:comment xml:lang="pt">Atualiza ou sobrepõe mensagens anteriores identificadas por uma referência.</rdfs:comment>
    <rdfs:comment xml:lang="en">Updates and supercedes the earlier message(s) identified in &lt;references&gt;</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#MsgType"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Health">
    <rdfs:comment xml:lang="pt">Saúde pública</rdfs:comment>
    <rdfs:comment xml:lang="en">Medical and public health</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Category"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Public">
    <rdfs:comment xml:lang="pt">Para a divulgação para o público em geral sem restrições</rdfs:comment>
    <rdfs:comment xml:lang="en">For general dissemination to unrestricted audiences.</rdfs:comment>
    <owl:disjointWith rdf:resource="#Restricted"/>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Scope"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Immediate">
    <rdfs:comment xml:lang="pt">Ações de resposta devem ser realizadas imediatamente.</rdfs:comment>
    <rdfs:comment xml:lang="en">Responsive action SHOULD be taken immediately.</rdfs:comment>
    <owl:disjointWith rdf:resource="#UrgencyUnknown"/>
    <owl:disjointWith>
      <owl:Class rdf:ID="Past"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Urgency"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="AllClear">
    <owl:disjointWith>
      <owl:Class rdf:ID="Assess"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Evacuate"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#None"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Shelter"/>
    </owl:disjointWith>
    <rdfs:comment xml:lang="en">The subject event no longer poses a threat or concern and any follow on action is described in &lt;instruction&gt;</rdfs:comment>
    <rdfs:comment xml:lang="pt">O evento não consiste mais em uma ameaça ou preocupação e qualquer ação a se tomar é descrita através de instruções.</rdfs:comment>
    <owl:disjointWith rdf:resource="#Avoid"/>
    <owl:disjointWith rdf:resource="#Monitor"/>
    <owl:disjointWith rdf:resource="#Prepare"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Execute"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#ResponseType"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="CertaintyUnknown">
    <rdfs:comment xml:lang="en">Certainty unknown</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Certainty"/>
  </owl:Class>
  <owl:Class rdf:ID="Transport">
    <rdfs:comment xml:lang="pt">Transporte público e privado.</rdfs:comment>
    <rdfs:comment xml:lang="en">Public and private transportation</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Category"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Observed">
    <rdfs:comment xml:lang="en">Determined to have occurred or to be ongoing</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Certainty"/>
  </owl:Class>
  <owl:Class rdf:about="#Moderate">
    <rdfs:comment xml:lang="pt">Possível ameaça a vida ou propriedade.</rdfs:comment>
    <rdfs:comment xml:lang="en">Possible threat to life or property</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:about="#SeverityUnknown"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Severe"/>
    </owl:disjointWith>
    <rdfs:subClassOf rdf:resource="#Severity"/>
  </owl:Class>
  <owl:Class rdf:about="#System">
    <rdfs:comment xml:lang="pt">Para mensagens que apoiam funções internas da rede de alerta.</rdfs:comment>
    <rdfs:comment xml:lang="en">For messages that support alert network internal functions.</rdfs:comment>
    <owl:disjointWith rdf:resource="#Test"/>
    <rdfs:subClassOf rdf:resource="#Status"/>
  </owl:Class>
  <owl:Class rdf:about="#CancelType">
    <rdfs:comment xml:lang="pt">Cancela as mensagens anteriores identificadas por uma referência.</rdfs:comment>
    <rdfs:comment xml:lang="en">Cancels the earlier message(s) identified in &lt;references&gt;

</rdfs:comment>
    <owl:disjointWith rdf:resource="#UpdateType"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#ErrorType"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#MsgType"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Possible">
    <rdfs:comment xml:lang="en">Possible but not likely (p &lt;= ~50%)</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Certainty"/>
  </owl:Class>
  <owl:Class rdf:about="#Past">
    <rdfs:comment xml:lang="pt">Ações de resposta não são mais necessárias.</rdfs:comment>
    <rdfs:comment xml:lang="en">Responsive action is no longer required</rdfs:comment>
    <owl:disjointWith rdf:resource="#UrgencyUnknown"/>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Urgency"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Env">
    <rdfs:comment xml:lang="pt">Poluição e outros alerta ambientais</rdfs:comment>
    <rdfs:comment xml:lang="en">Pollution and other environmental</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Category"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Resource">
    <rdfs:comment xml:lang="en">Refers to an additional file with supplemental information related to this &lt;info&gt; element; e.g., an image or audio file</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="Future">
    <rdfs:comment xml:lang="pt">Ações de resposta devem ser tomadas em um futuro próximo.</rdfs:comment>
    <rdfs:comment xml:lang="en">Responsive action SHOULD be taken in the near future</rdfs:comment>
    <owl:disjointWith rdf:resource="#UrgencyUnknown"/>
    <owl:disjointWith rdf:resource="#Past"/>
    <owl:disjointWith rdf:resource="#Immediate"/>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Urgency"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Assess">
    <owl:disjointWith rdf:resource="#None"/>
    <owl:disjointWith rdf:resource="#Monitor"/>
    <rdfs:comment xml:lang="pt">Avaliar a informação na mensagem.


</rdfs:comment>
    <rdfs:comment xml:lang="en">Evaluate the information in this message</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:about="#Evacuate"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Shelter"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Execute"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#ResponseType"/>
    </rdfs:subClassOf>
    <owl:disjointWith rdf:resource="#Prepare"/>
    <owl:disjointWith rdf:resource="#Avoid"/>
  </owl:Class>
  <owl:Class rdf:about="#Scope">
    <rdfs:comment xml:lang="pt">Refere-se a intenção da distribuição da mensagem de alerta.</rdfs:comment>
    <rdfs:comment xml:lang="en">Refers to the intended distribution of the alert message.

</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="Safety">
    <rdfs:comment xml:lang="pt">Emergência geral e segurança pública

</rdfs:comment>
    <rdfs:comment xml:lang="en">General emergency and public safety</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Category"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Category">
    <rdfs:comment xml:lang="pt">Refere-se a categoria do assunto do evento do alerta de mensagem.</rdfs:comment>
    <rdfs:comment xml:lang="en">Refers to the category of the subject event of the alert message.

</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#Urgency">
    <rdfs:comment xml:lang="en">The urgency of the subject event of the alert message</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="OtherCategory">
    <rdfs:comment xml:lang="pt">Outros eventos</rdfs:comment>
    <rdfs:comment xml:lang="en">Others events</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Category"/>
  </owl:Class>
  <owl:Class rdf:ID="Likely">
    <rdfs:comment xml:lang="en">Likely (p &gt; ~50%)</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Certainty"/>
  </owl:Class>
  <owl:Class rdf:about="#Evacuate">
    <rdfs:comment xml:lang="pt">Realocação de acordo a instruções
</rdfs:comment>
    <rdfs:comment xml:lang="en">Relocate as instructed in the &lt;instruction&gt;</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:about="#Shelter"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Prepare"/>
    <owl:disjointWith rdf:resource="#None"/>
    <owl:disjointWith rdf:resource="#Monitor"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Execute"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#ResponseType"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Severe">
    <rdfs:comment xml:lang="pt">Significante ameaça a vida ou propriedade.</rdfs:comment>
    <rdfs:comment xml:lang="en">Significant threat to life or property</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:about="#SeverityUnknown"/>
    </owl:disjointWith>
    <rdfs:subClassOf rdf:resource="#Severity"/>
  </owl:Class>
  <owl:Class rdf:ID="AckType">
    <rdfs:comment xml:lang="pt">Reconhece a recepção e aceitação da mensagem identificada previamente.</rdfs:comment>
    <rdfs:comment xml:lang="en">Acknowledges receipt and acceptance of the message(s) identified in &lt;references&gt;.</rdfs:comment>
    <owl:disjointWith rdf:resource="#UpdateType"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#ErrorType"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#CancelType"/>
    <owl:disjointWith rdf:resource="#AlertType"/>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#MsgType"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Infra">
    <rdfs:comment xml:lang="pt">Utilidades, telecomunicações, outras infraestruturadas não transportadas</rdfs:comment>
    <rdfs:comment xml:lang="en">Utility, telecommunication, other non-transport infrastructure</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Category"/>
  </owl:Class>
  <owl:Class rdf:ID="Met">
    <rdfs:comment xml:lang="pt">Metereológica (ex:enchente)

</rdfs:comment>
    <rdfs:comment xml:lang="en">Meteorological (inc. flood)</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Category"/>
  </owl:Class>
  <owl:Class rdf:ID="CBRNE">
    <rdfs:comment xml:lang="pt">Ataques e ameaças químicas, biologicas, radiológicas, nucleares ou de explosivos.</rdfs:comment>
    <rdfs:comment xml:lang="en">Chemical, Biological, Radiological, Nuclear or High-Yield Explosive threat or attack</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Category"/>
  </owl:Class>
  <owl:Class rdf:ID="AlertMessage">
    <rdfs:comment xml:lang="pt">Refere-se todos os componentes da uma mensagem de alerta.  </rdfs:comment>
    <rdfs:comment xml:lang="en">Refers to all component parts of the alert message.</rdfs:comment>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:someValuesFrom>
              <owl:Class rdf:ID="Incident"/>
            </owl:someValuesFrom>
            <owl:onProperty>
              <owl:ObjectProperty rdf:ID="hasIncidentRelated"/>
            </owl:onProperty>
          </owl:Restriction>
          <owl:Restriction>
            <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
            >1</owl:maxQualifiedCardinality>
            <owl:onClass rdf:resource="#Scope"/>
            <owl:onProperty>
              <owl:FunctionalProperty rdf:ID="hasScope"/>
            </owl:onProperty>
          </owl:Restriction>
          <owl:Restriction>
            <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
            >1</owl:maxQualifiedCardinality>
            <owl:onClass rdf:resource="#Status"/>
            <owl:onProperty>
              <owl:FunctionalProperty rdf:ID="hasStatus"/>
            </owl:onProperty>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:ID="Circle">
    <rdfs:comment xml:lang="pt">Os valores emparelhados de um ponto e o raio que delineam a área afetada da mensagem de alerta.</rdfs:comment>
    <rdfs:comment xml:lang="en">The paired values of a point and radius delineating the affected area of the alert message.</rdfs:comment>
    <owl:disjointWith rdf:resource="#Polygon"/>
    <rdfs:subClassOf rdf:resource="#Area"/>
  </owl:Class>
  <owl:Class rdf:ID="Extreme">
    <rdfs:comment xml:lang="pt">Extrema ameaça a vida ou propriedade.</rdfs:comment>
    <rdfs:comment xml:lang="en">Extraordinary threat to life or property</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:about="#SeverityUnknown"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Severe"/>
    <owl:disjointWith rdf:resource="#Moderate"/>
    <owl:disjointWith rdf:resource="#Minor"/>
    <rdfs:subClassOf rdf:resource="#Severity"/>
  </owl:Class>
  <owl:Class rdf:ID="Expected">
    <rdfs:comment xml:lang="pt">Ações de resposta devem ser realizadas logo (dentro da próxima hora).</rdfs:comment>
    <rdfs:comment xml:lang="en">Responsive action SHOULD be taken soon (within next hour)</rdfs:comment>
    <owl:disjointWith rdf:resource="#UrgencyUnknown"/>
    <owl:disjointWith rdf:resource="#Past"/>
    <owl:disjointWith rdf:resource="#Immediate"/>
    <owl:disjointWith rdf:resource="#Future"/>
    <rdfs:subClassOf rdf:resource="#Urgency"/>
  </owl:Class>
  <owl:Class rdf:about="#Incident">
    <rdfs:comment xml:lang="pt">Termo que se refere a ocorrências de qualquer escala que pode exigir alguma forma de gerenciamento e resposta à emergência, e que requer acompanhamento e troca de informação. No Sistema Nacional de Gerenciamento de Emergência americano (NIMS), "Incidente" é visto como uma declaração formal ou informal de emergência ou desastre por uma organização de nível local, estadual, federal ou por uma jurisdição.</rdfs:comment>
    <rdfs:comment xml:lang="en">Term referring to occurrences of any scale that may require some form of Emergency Response and Management, and that requires tracking and information exchange. 
“Incident” is viewed from the NIMS Emergency Management perspective as a formal or informal declaration of emergency or disaster by an organization at the state, local, federal level or by a jurisdiction.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#MsgType">
    <rdfs:comment xml:lang="pt">Refere-se a natureza da mensagem de alerta.</rdfs:comment>
    <rdfs:comment xml:lang="en">Refers to the nature of the alert message.

</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#Execute">
    <rdfs:comment xml:lang="pt">Executar uma atividade pré-planejada identificada em instruções</rdfs:comment>
    <rdfs:comment xml:lang="en">Execute a pre-planned activity identified in &lt;instruction&gt;</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:about="#Shelter"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Prepare"/>
    <owl:disjointWith rdf:resource="#None"/>
    <owl:disjointWith rdf:resource="#Monitor"/>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#ResponseType"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#SeverityUnknown">
    <rdfs:comment xml:lang="en">Severity unknown</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Severity"/>
  </owl:Class>
  <owl:Class rdf:about="#ResponseType">
    <rdfs:comment xml:lang="pt">O tipo de ação recomendada para a audiência alvo da mensagem de alerta.</rdfs:comment>
    <rdfs:comment xml:lang="en">The code denoting the type of action recommended for the target audience</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#Shelter">
    <rdfs:comment xml:lang="pt">Se abrigar num local ou por instrução
</rdfs:comment>
    <rdfs:comment xml:lang="en">Take shelter in place or per &lt;instruction&gt;
</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#ResponseType"/>
  </owl:Class>
  <owl:Class rdf:about="#ErrorType">
    <rdfs:comment xml:lang="pt">Indica rejeição a um mensagem identificada por uma referência.</rdfs:comment>
    <rdfs:comment xml:lang="en">Indicates rejection of the message(s) identified in &lt;references&gt;; explanation SHOULD appear in &lt;note&gt;</rdfs:comment>
    <owl:disjointWith rdf:resource="#UpdateType"/>
    <rdfs:subClassOf rdf:resource="#MsgType"/>
  </owl:Class>
