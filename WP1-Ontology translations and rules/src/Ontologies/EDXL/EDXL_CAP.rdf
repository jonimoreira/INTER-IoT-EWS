        <rdfs:range rdf:resource="&edxl_cap;Info"/>
    </owl:ObjectProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#hasMsgType -->

    <owl:ObjectProperty rdf:about="&edxl_cap;hasMsgType">
