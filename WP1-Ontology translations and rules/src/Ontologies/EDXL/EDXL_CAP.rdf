    <owl:ObjectProperty rdf:about="&edxl_cap;isAreaOf"/>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_cap#isCategoryOf -->

    <owl:ObjectProperty rdf:about="&edxl_cap;isCategoryOf">
