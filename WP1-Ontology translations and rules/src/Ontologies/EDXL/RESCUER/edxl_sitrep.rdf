    </owl:Class>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_sitrep#Mitigation -->

    <owl:Class rdf:about="&sitrep;Mitigation">
        <rdfs:subClassOf rdf:resource="&sitrep;LifecyclePhase"/>
