        <rdfs:comment xml:lang="en">In-route or arrived at an Emergency Department or Hospital</rdfs:comment>
        <rdfs:comment xml:lang="pt">A caminho ou que acabou de chegar a um Departamento de Emergência ou Hospital. </rdfs:comment>
    </owl:Class>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_sitrep#HumanLifeAndSafetyThreat -->

    <owl:Class rdf:about="&sitrep;HumanLifeAndSafetyThreat">
