        <owl:disjointWith rdf:resource="&sitrep;Recovery"/>
        <owl:disjointWith rdf:resource="&sitrep;Response"/>
    </owl:Class>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_sitrep#Moderate -->

    <owl:Class rdf:about="&sitrep;Moderate">
        <rdfs:subClassOf rdf:resource="&sitrep;IncidentComplexity"/>
        <owl:disjointWith rdf:resource="&sitrep;ModerateComplex"/>
        <rdfs:comment xml:lang="en">Public / Professional preparedness is high, Coordination Complexity and involvement is moderate (local, regional)</rdfs:comment>
        <rdfs:comment xml:lang="pt">Preparação profissional e do público é alta, complexidade de coordenação e envolvimento é moderada (local, regional).</rdfs:comment>
    </owl:Class>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_sitrep#ModerateComplex -->

    <owl:Class rdf:about="&sitrep;ModerateComplex">
        <rdfs:subClassOf rdf:resource="&sitrep;IncidentComplexity"/>
        <rdfs:comment xml:lang="en">Public / Professional preparedness is moderate-high, Coordination Complexity and involvement is high (local, regional, state, possibly national).</rdfs:comment>
        <rdfs:comment xml:lang="pt">Preparação profissional e do público é moderada-alta, complexidade de coordenação e envolvimento é alta (local, regional, estado, possivelmente nacional).</rdfs:comment>
    </owl:Class>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_sitrep#NoConfidence -->

    <owl:Class rdf:about="&sitrep;NoConfidence">
        <rdfs:subClassOf rdf:resource="&sitrep;Confidence"/>
