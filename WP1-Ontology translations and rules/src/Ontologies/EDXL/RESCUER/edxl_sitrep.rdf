    <!-- http://fpc.ufba.br/ontologies/edxl_sitrep#SituationSummary -->

    <owl:Class rdf:about="&sitrep;SituationSummary">
        <rdfs:comment xml:lang="en">Provides concise status and descriptive information about the overall situation, primarily as input to human decision-making across coordinating organizations and up the chain of command .</rdfs:comment>
    </owl:Class>
    


