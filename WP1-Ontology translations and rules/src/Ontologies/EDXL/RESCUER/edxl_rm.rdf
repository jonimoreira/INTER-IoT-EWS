        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_rm#isOwnerOf -->

    <owl:ObjectProperty rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#isOwnerOf">
