    <!-- http://fpc.ufba.br/ontologies/edxl_rm#isLocationOf -->

    <owl:ObjectProperty rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#isLocationOf"/>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_rm#isMessageRecallOf -->

    <owl:ObjectProperty rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#isMessageRecallOf">
