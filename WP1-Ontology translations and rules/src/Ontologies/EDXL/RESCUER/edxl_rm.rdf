    <!-- http://fpc.ufba.br/ontologies/edxl_rm#rights -->

    <owl:AnnotationProperty rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#rights"/>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_rm#status -->

