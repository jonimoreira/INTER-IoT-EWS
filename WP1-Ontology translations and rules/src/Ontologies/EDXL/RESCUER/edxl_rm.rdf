        <rdfs:range rdf:resource="http://fpc.ufba.br/ontologies/edxl_rm#ResponseInformation"/>
        <owl:inverseOf rdf:resource="http://fpc.ufba.br/ontologies/edxl_rm#isResponseInformationOf"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_rm#hasResponsibleParty -->

    <owl:ObjectProperty rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#hasResponsibleParty">
