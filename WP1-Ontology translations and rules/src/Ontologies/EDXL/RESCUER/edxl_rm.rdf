     -->

    


    <!-- http://fpc.ufba.br/ontologies/edxl_rm#hasAssignment -->

    <owl:ObjectProperty rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#hasAssignment">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="http://fpc.ufba.br/ontologies/edxl_rm#Assignment"/>
        <rdfs:domain rdf:resource="http://fpc.ufba.br/ontologies/edxl_rm#ResourceInformation"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_rm#hasContact -->

    <owl:ObjectProperty rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#hasContact">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment xml:lang="en">The contact associated with the resource message</rdfs:comment>
        <rdfs:range rdf:resource="http://fpc.ufba.br/ontologies/edxl_rm#Contact"/>
        <rdfs:domain rdf:resource="http://fpc.ufba.br/ontologies/edxl_rm#ResourceMessage"/>
        <owl:inverseOf rdf:resource="http://fpc.ufba.br/ontologies/edxl_rm#isContactOf"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_rm#hasFunding -->

    <owl:ObjectProperty rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#hasFunding">
        <rdfs:comment xml:lang="en">The funds that will pay for the resource</rdfs:comment>
        <rdfs:range rdf:resource="http://fpc.ufba.br/ontologies/edxl_rm#Funding"/>
        <owl:inverseOf rdf:resource="http://fpc.ufba.br/ontologies/edxl_rm#isFundingOf"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#CommitResource"/>
                    <rdf:Description rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#ReleaseResource"/>
                    <rdf:Description rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#ReportResourceDeploymentStatus"/>
                    <rdf:Description rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#RequestExtendedDeploymentDuration"/>
                    <rdf:Description rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#RequestInformation"/>
                    <rdf:Description rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#RequestQuote"/>
                    <rdf:Description rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#RequestResource"/>
                    <rdf:Description rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#RequestResourceDeploymentStatus"/>
                    <rdf:Description rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#RequestReturn"/>
                    <rdf:Description rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#RequisitionResource"/>
                    <rdf:Description rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#ResponseToRequestExtendedDeploymentDuration"/>
                    <rdf:Description rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#ResponseToRequestInformation"/>
                    <rdf:Description rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#ResponseToRequestQuote"/>
                    <rdf:Description rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#ResponseToRequestResource"/>
                    <rdf:Description rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#ResponseToRequestReturn"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_rm#hasIncidentInformation -->

    <owl:ObjectProperty rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#hasIncidentInformation">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="http://fpc.ufba.br/ontologies/edxl_rm#Incident"/>
        <rdfs:domain rdf:resource="http://fpc.ufba.br/ontologies/edxl_rm#ResourceMessage"/>
        <owl:inverseOf rdf:resource="http://fpc.ufba.br/ontologies/edxl_rm#isIncidentInformationOf"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_rm#hasJurisdiction -->

    <owl:ObjectProperty rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#hasJurisdiction">
        <rdfs:range rdf:resource="http://fpc.ufba.br/ontologies/edxl_rm#Jurisdiction"/>
        <rdfs:domain rdf:resource="http://fpc.ufba.br/ontologies/edxl_rm#Ownership"/>
    </owl:ObjectProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_rm#hasLocation -->

    <owl:ObjectProperty rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#hasLocation">
        <rdfs:range rdf:resource="http://fpc.ufba.br/ontologies/edxl_rm#Location"/>
        <owl:inverseOf rdf:resource="http://fpc.ufba.br/ontologies/edxl_rm#isLocationOf"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#Contact"/>
                    <rdf:Description rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#Schedule"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_rm#hasMessageRecall -->

    <owl:ObjectProperty rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#hasMessageRecall">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment xml:lang="en">MessageRecall is used to replace a previously sent message by updating or canceling it.</rdfs:comment>
        <rdfs:range rdf:resource="http://fpc.ufba.br/ontologies/edxl_rm#MessageRecall"/>
        <rdfs:domain rdf:resource="http://fpc.ufba.br/ontologies/edxl_rm#ResourceMessage"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_rm#hasOwner -->

    <owl:ObjectProperty rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#hasOwner">
        <rdfs:comment xml:lang="en">An agency or supplier that owns the resource (which may not be the home unit or dispatch). </rdfs:comment>
        <rdfs:comment xml:lang="pt">Uma agência ou fornecedor que é dono do recurso (que pode não ser a unidade responsável por manter a informação sobre o recurso)</rdfs:comment>
        <rdfs:range rdf:resource="http://fpc.ufba.br/ontologies/edxl_rm#Ownership"/>
        <rdfs:domain rdf:resource="http://fpc.ufba.br/ontologies/edxl_rm#Resource"/>
    </owl:ObjectProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_rm#hasResource -->

    <owl:ObjectProperty rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#hasResource">
        <rdfs:range rdf:resource="http://fpc.ufba.br/ontologies/edxl_rm#Resource"/>
        <rdfs:domain rdf:resource="http://fpc.ufba.br/ontologies/edxl_rm#ResourceInformation"/>
    </owl:ObjectProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_rm#hasResourceInformation -->

    <owl:ObjectProperty rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#hasResourceInformation">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="http://fpc.ufba.br/ontologies/edxl_rm#ResourceInformation"/>
        <rdfs:domain rdf:resource="http://fpc.ufba.br/ontologies/edxl_rm#ResourceMessage"/>
        <owl:inverseOf rdf:resource="http://fpc.ufba.br/ontologies/edxl_rm#isResourceInformationOf"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_rm#hasResourceStatus -->

    <owl:ObjectProperty rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#hasResourceStatus">
        <rdfs:domain rdf:resource="http://fpc.ufba.br/ontologies/edxl_rm#Resource"/>
        <rdfs:range rdf:resource="http://fpc.ufba.br/ontologies/edxl_rm#ResourceStatus"/>
        <owl:inverseOf rdf:resource="http://fpc.ufba.br/ontologies/edxl_rm#isResourceStatusOf"/>
    </owl:ObjectProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_rm#hasResponseInformation -->

    <owl:ObjectProperty rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#hasResponseInformation">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
