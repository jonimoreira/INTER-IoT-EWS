        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_rm#owner -->

    <owl:DatatypeProperty rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#owner">
