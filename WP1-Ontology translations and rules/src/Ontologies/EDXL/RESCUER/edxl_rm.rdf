    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_rm#deploymentStatus -->

    <owl:DatatypeProperty rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#deploymentStatus">
        <rdfs:comment xml:lang="en">Any value from a discrete managed list, used to specify the general state of a resource if known</rdfs:comment>
        <rdfs:domain rdf:resource="http://fpc.ufba.br/ontologies/edxl_rm#ResourceStatus"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


