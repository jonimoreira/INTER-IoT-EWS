                    <rdf:Description rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#RequestReturn"/>
                    <rdf:Description rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#RequisitionResource"/>
                    <rdf:Description rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#ResponseToRequestExtendedDeploymentDuration"/>
                    <rdf:Description rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#ResponseToRequestInformation"/>
                    <rdf:Description rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#ResponseToRequestQuote"/>
                    <rdf:Description rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#ResponseToRequestResource"/>
                    <rdf:Description rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#ResponseToRequestReturn"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:DatatypeProperty>
    


