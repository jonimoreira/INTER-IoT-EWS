        <rdfs:domain rdf:resource="http://fpc.ufba.br/ontologies/edxl_rm#Resource"/>
    </owl:ObjectProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_rm#hasScheduleInformation -->

    <owl:ObjectProperty rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#hasScheduleInformation">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
