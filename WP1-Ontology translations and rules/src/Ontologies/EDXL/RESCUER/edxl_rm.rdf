    <!-- http://fpc.ufba.br/ontologies/edxl_rm#additionalContactInformation -->

    <owl:DatatypeProperty rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#additionalContactInformation">
        <rdfs:comment xml:lang="en">Any other contact information including name and other Party information.</rdfs:comment>
        <rdfs:domain rdf:resource="http://fpc.ufba.br/ontologies/edxl_rm#Contact"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_rm#anticipatedFunction -->

    <owl:DatatypeProperty rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#anticipatedFunction">
        <rdfs:comment xml:lang="en">Anticipated function, task, job, or role to be provided by the requested resource.</rdfs:comment>
        <rdfs:comment xml:lang="pt">Função antecipada, tarefa, job ou função a ser fornecida pelo recurso solicitado.</rdfs:comment>
        <rdfs:domain rdf:resource="http://fpc.ufba.br/ontologies/edxl_rm#Assignment"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_rm#availability -->

    <owl:DatatypeProperty rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#availability">
        <rdfs:comment xml:lang="en">Text to describe availability and limitations on availability. Resource availability refers to resource that it is present or ready for immediate use, or otherwise accessible or obtainable, or is qualified or willing to do something or to assume a responsibility.</rdfs:comment>
        <rdfs:domain rdf:resource="http://fpc.ufba.br/ontologies/edxl_rm#ResourceStatus"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_rm#contactDescription -->

    <owl:DatatypeProperty rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#contactDescription">
        <rdfs:comment xml:lang="en">Identifying information associated with a contact role related to the resource message</rdfs:comment>
        <rdfs:domain rdf:resource="http://fpc.ufba.br/ontologies/edxl_rm#Contact"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_rm#contactRadioChannel -->

    <owl:DatatypeProperty rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#contactRadioChannel">
        <rdfs:comment xml:lang="en">Contact radio channel of the person or organization referred to in ContactRole.</rdfs:comment>
        <rdfs:domain rdf:resource="http://fpc.ufba.br/ontologies/edxl_rm#Contact"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_rm#contactRadioType -->

    <owl:DatatypeProperty rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#contactRadioType">
        <rdfs:comment xml:lang="en">Contact radio type of the person or organization referred to in ContactRole.</rdfs:comment>
        <rdfs:domain rdf:resource="http://fpc.ufba.br/ontologies/edxl_rm#Contact"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_rm#contactRole -->

    <owl:DatatypeProperty rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#contactRole">
        <rdfs:comment xml:lang="en">Role of the contact associated with the resource message.</rdfs:comment>
        <rdfs:domain rdf:resource="http://fpc.ufba.br/ontologies/edxl_rm#Contact"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://fpc.ufba.br/ontologies/edxl_rm#dateTime -->

    <owl:DatatypeProperty rdf:about="http://fpc.ufba.br/ontologies/edxl_rm#dateTime">
