            rdfs:seeAlso "http://www.omgsysml.org/qudv#Unit" ;
            
            dc:source "skos:exactMatch 'factor' [SysML 1.2-QUDV] http://www.omgwiki.org/OMGSysML/doku.php?id=sysml-qudv:quantities_units_dimensions_values_qudv" ;
            
