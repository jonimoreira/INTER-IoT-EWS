               rdfs:label "unitComponent" ;
               
               dc:source "skos:exactMatch 'unit' [SysML 1.2-QUDV] http://www.omgwiki.org/OMGSysML/doku.php?id=sysml-qudv:quantities_units_dimensions_values_qudv" ;
               
